{
    "name": "sierratecnologia/facilitador",
    "description": "Facilitador para ajudar no desenvolvimento de pÃ¡ginas web",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Rebello Sierra",
            "email": "ricardo@sierratecnologia.com.br"
        }
    ],
    "require": {
        "php": ">=7.0",
        "arrilot/laravel-widgets": "^3.0",
        "sierratecnologia/facilitador-hooks": "dev-master",
        "sierratecnologia/integrations": "dev-master",
        "ext-json": "*",
        "illuminate/config": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/http": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/console": "^5.0|^6.0|^7.0|^8.0",
        "laravel/framework": "^5.8|^6.0|^7.0|^8.0",
        "elasticquent/elasticquent": "*",
        "waavi/sanitizer": "*",
        "yajra/laravel-datatables-oracle": "^9.0",
        "spatie/laravel-pjax": ">=1.3",
        "laravel/passport": "^7.0|^8.0|^9.0",
        "league/plates": "^1.0|^2.0|^3.0|^4.0",
        "jeroennoten/laravel-adminlte": "*",
        "sierratecnologia/tracking": "dev-master",
        "sierratecnologia/audit": "dev-master",
        "sierratecnologia/laravel-support": "dev-master",
        "sierratecnologia/muleta": "dev-master",
        "sierratecnologia/stalker": "dev-master",
        "ricardosierra/laravel-model-events": "^1.0",
        "cmgmyr/messenger": "^2.0|^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0",
        "mockery/mockery": ">=0.9.4",
        "mikey179/vfsstream": ">=1.6",
        "orchestra/database": ">=3.4",
        "orchestra/testbench": ">=3.4"
    },
    "autoload": {
        "psr-4": {
            "Facilitador\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Facilitador\\FacilitadorProvider"
            ],
            "dont-discover": [
                "scaffold-interface"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
