    },
    "autoload": {
        "psr-4": {
            "Facilitador\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Facilitador\\FacilitadorProvider"
            ],
            "dont-discover": [
                "scaffold-interface"
            ]
        }
