{
    "name": "sierratecnologia/facilitador",
    "description": "Recursos para Desenvolvimento Complexo",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Rebello Sierra",
            "email": "ricardo@sierratecnologia.com.br"
        }
    ],
    "require": {
        "php": ">=7.0",
        "doctrine/dbal": ">=2.5",
        "illuminate/config": "^5.8",
        "illuminate/database": "^5.8",
        "illuminate/http": "^5.8",
        "illuminate/support": "^5.8",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "yajra/laravel-datatables-oracle": "~9.0",
        "spatie/laravel-pjax": "^1.3",
        "venturecraft/revisionable": "^1.30",
        "ricardosierra/validate": "^0.2",
        "laracasts/presenter": "0.2.*",
        "laravelcollective/html": "^5.8",
        "jeroennoten/laravel-adminlte": "*",
        "league/csv": "^7.1",
        "ramsey/uuid": "^3.8",
        "zendframework/zend-crypt": "*",
        "maatwebsite/excel": "^3.1",
        "sabre/xml": "*",
        "larapack/hooks": "^1.0.5",

        "nicolaslopezj/searchable": "^1.10",
        "sierratecnologia/crypto": "dev-master",
        "ricardosierra/minify": "dev-master",
        "cipemotion/medialibrary": "*",
        "laracasts/flash": "*",
        "google/recaptcha": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0",
        "mockery/mockery": ">=0.9.4",
        "mikey179/vfsstream": ">=1.6",
        "orchestra/database": ">=3.4",
        "orchestra/testbench": ">=3.4"
    },
    "autoload": {
        "psr-4": {
            "Facilitador\\": "src/"
        },
        "files": [
            "src/Helpers/form_maker.php",
            "src/Helpers/input_maker.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Facilitador\\FacilitadorProvider"
            ],
            "dont-discover": [
                "scaffold-interface"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
