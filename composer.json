{
    "name": "sierratecnologia/facilitador",
    "description": "Facilitador para ajudar no desenvolvimento de pÃ¡ginas web",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "facilitador"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/facilitador/",
    "support": {
        "email": "help@sierratecnologia.com",
        "issues": "https://github.com/sierratecnologia/facilitador/issues",
        "source": "https://github.com/sierratecnologia/facilitador",
        "docs": "https://github.com/sierratecnologia/facilitador/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        }
    ],
    "require": {
        "php": "^7.4",
        "arrilot/laravel-widgets": "^3.0",
        "sierratecnologia/facilitador-hooks": "dev-master",
        "sierratecnologia/integrations": "dev-master",
        "ext-json": "*",
        "illuminate/config": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/console": "^7.0|^8.0",
        "laravel/framework": "^7.0|^8.0",
        "socialiteproviders/manager": "~2.0 || ~3.0",
        "socialiteproviders/github": "dev-master",
        "socialiteproviders/jira": ">=2.1",
        "socialiteproviders/linkedin": ">=3.0",
        "sierratecnologia/tracking": "dev-master",
        "sierratecnologia/transmissor": "dev-master",
        "sierratecnologia/audit": "dev-master",
        "sierratecnologia/laravel-support": "dev-master",
        "sierratecnologia/muleta": "dev-master",
        "sierratecnologia/stalker": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0",
        "mockery/mockery": ">=0.9.4",
        "mikey179/vfsstream": ">=1.6",
        "orchestra/database": ">=3.4",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Facilitador\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Facilitador\\FacilitadorProvider"
            ],
            "dont-discover": [
                "scaffold-interface"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
